{
  "name": "shared-goodcity",
  "version": "0.0.0",
  "description": "The default blueprint for ember-cli addons.",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "start": "ember server",
    "build": "ember build",
    "test": "ember test",
    "testall": "ember try:testall",
    "test:ci": "EMBER_CLI_CORDOVA=0 ember-pretty-test"

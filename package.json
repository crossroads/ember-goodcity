  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,json,css,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "repository": {
    "url": "https://github.com/crossroads/shared.goodcity.git",
    "private": true
  },
  "engines": {
    "node": ">= 6"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "broccoli-asset-rev": "^3.0.0",
    "ember-ajax": "5.0.0",
    "ember-cli": "2.18.0",
    "ember-cli-app-version": "^3.2.0",
    "ember-cli-code-coverage": "^0.4.2",
    "ember-cli-content-security-policy": "1.1.1",
    "ember-cli-dependency-checker": "^3.2.0",
    "ember-cli-htmlbars": "^2.0.3",
    "ember-cli-htmlbars-inline-precompile": "^1.0.0",
    "ember-cli-inject-live-reload": "^2.0.2",
    "ember-cli-jshint": "^1.0.0",
    "ember-cli-qunit": "2.2.6",
    "ember-cli-release": "^1.0.0-beta.2",
    "ember-cli-rollbar": "0.3.1 ",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-test-loader": "2.2.0",
    "ember-cli-uglify": "^3.0.0",
    "ember-data": "3.27.1",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-disable-proxy-controllers": "^1.0.2",
    "ember-export-application-global": "^2.0.1",
    "ember-i18n": "~5.3.1",
    "ember-load-initializers": "^2.1.2",
    "ember-pretty-test": "^1.0.4",
    "ember-resolver": "^5.3.0",
    "ember-try": "1.4.0",
    "ember-welcome-page": "^3.1.1",
    "husky": "^3.0.9",
    "lint-staged": "^9.4.2",
    "loader.js": "4.7.0",
    "prettier": "2.3.1"
  },
  "keywords": [
    "ember-addon",
    "goodcity"
  ],
  "dependencies": {
    "cryptiles": "^4.1.2",
    "ember-cli-babel": "6.17.0",
    "ember-cli-coffeescript": "^1.16.1",
